{
  "name": "starwars-names-dummy",
  "version": "0.0.0-semantically-released",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "build": "babel --out-dir dist --ignore *.test.js src",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardoerikson/starwars-names-dummy.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Ricardo Erikson <ricardoerikson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardoerikson/starwars-names-dummy/issues"
  },
  "homepage": "https://github.com/ricardoerikson/starwars-names-dummy#readme",
  "dependencies": {
    "lodash": "4.14.0",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.3.2",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
